    "@types/minimist": "^1.2.2",
    "@types/node": "^18.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "minimist": "^1.2.6",
    "tslib": "^2.4.0"
  }
}
