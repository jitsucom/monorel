{
  "name": "monorel",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc -p .",
    "code-style:check": "prettier --check --config ./.prettierrc.json --ignore-path ./.prettierignore .",
    "code-style:fix": "prettier --write --config ./.prettierrc.json --ignore-path ./.prettierignore .",
    "monorel": "./bin/monorel",
    "release:publish": "pnpm monorel --tag latest --filter './packages/a'  --filter './packages/b'",
    "canary:publish": "pnpm monorel --version '0.1.0-alpha.{rev}' --tag canary --publish",
    "ci": "pnpm clean && pnpm build && pnpm canary:publish"
  },
  "files": [
    "dist",
    "bin"
  ],
  "keywpords": [],
